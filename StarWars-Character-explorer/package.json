{
  "name": "starwars-character-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "run-s build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "lint": "run-s lint:eslint:fix lint:stylelint:fix lint:prettier:fix",
    "lint:prettier": "prettier --check \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:prettier:fix": "prettier --write \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:eslint": "eslint --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix"
  },
  "dependencies": {
    "@cssninja/bulma-css-vars": "^0.9.2",
    "@unhead/vue": "^1.8.9",
    "axios": "^1.6.5",
    "bulma": "^0.9.4",
    "pinia": "^2.1.7",
    "terser": "^5.26.0",
    "unhead": "^1.8.9",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@unhead/addons": "^1.8.9",
    "@vitejs/plugin-vue": "^4.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-md": "^1.0.19",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-scss": "^6.0.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}
